{
  "name": "@pkmn/mods",
  "version": "0.3.17",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "exports": {
    ".": "./build/index.js",
    "./gen1stadium": "./build/gen1stadium/index.js",
    "./gen8dlc1": "./build/gen8dlc1/index.js",
    "./letsgo": "./build/letsgo/index.js",
    "./vgc17": "./build/vgc17/index.js"
  },
  "description": "Support for non-standard mods to @pkmn/sim and @pkmn/dex",
  "repository": "github:pkmn/ps",
  "author": "Guangcong Luo <guangcongluo@gmail.com> (http://guangcongluo.com)",
  "contributors": [
    "Cathy J. Fitzpatrick <cathy@cathyjf.com> (https://cathyjf.com)",
    "Bill Meltsner <bill@meltsner.com> (http://www.meltsner.com)"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@pkmn/dex-types": "^0.3.17"
  },
  "devDependencies": {
    "@pkmn/dex": "^0.3.17",
    "@pkmn/sim": "^0.3.17"
  },
  "scripts": {
    "lint": "eslint --cache src --ext ts",
    "fix": "eslint --fix src --ext ts",
    "compile": "tsc -p .",
    "build": "npm run compile",
    "test": "jest",
    "pretest": "npm run build",
    "posttest": "npm run lint"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "node_modules/",
      "build/"
    ]
  },
  "eslintConfig": {
    "extends": "@pkmn",
    "overrides": [
      {
        "files": [
          "./src/gen1stadium/*.ts",
          "./src/gen8dlc1/*.ts",
          "./src/letsgo/*.ts",
          "./src/vgc17/*.ts"
        ],
        "rules": {
          "@typescript-eslint/quotes": "off",
          "@typescript-eslint/indent": [
            "error",
            "tab",
            {
              "flatTernaryExpressions": true
            }
          ],
          "@typescript-eslint/no-unused-vars": "off",
          "max-len": "off"
        }
      }
    ]
  }
}
